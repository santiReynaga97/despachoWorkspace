{
  "name": "web-api-swagger",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "outputs": ["generated/web-api-swagger"]
    },
    "codegen": {
      "executor": "@nx-dotnet/core:openapi-codegen",
      "options": {
        "useOpenApiGenerator": true,
        "openApiGenerator": "typescript",
        "openapiJsonPath": "generated/web-api-swagger",
        "outputProject": "generated-web-api-types"
      },
      "dependsOn": ["swagger"],
      "inputs": ["{projectRoot}/generated/web-api-swagger"],
      "outputs": ["{workspaceRoot}"]
    }
  },
  "implicitDependencies": ["SrcManagement.WebApi"]
}
